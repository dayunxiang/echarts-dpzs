.container {
	/*height: 100%;*/
	width: 1600px;
	height: 1200px;
	background: url(../images/right/bg.png) center center no-repeat;
	background-size: 100% 100%;
	padding: 30px 20px 0 20px;
	overflow: auto;
	overflow: hidden;
}

.top {
	overflow: hidden;
	margin-bottom: 20px;
}

.monitor-box {
	float: left;
	width: 506px;
	height: 469px;
	margin-right: 20px;
	background: url(../images/right/monitor-border.png) center 45px no-repeat;
	background-size: 100% 424px;
}

.monitor-box:last-child {
	margin: 0;
}

.top-title {
	height: 45px;
	line-height: 45px;
}

.bottom {
	overflow: hidden;
	height: 634px;
}

.map-box, .other-box {
	float: left;
}

.map-box {
	position: relative;
	margin-right: 20px;
	width: 1032px;
	height: 100%;
	background-color: rgba(0, 105, 255, 0.2);
	
	/*padding: 10px 15px;*/
}

.map-title {
	height: 60px;
}

.map-title > div {
	height: 38px;
	line-height: 38px;
	margin-top: 10px;
	text-align: center;
	font-size: 20px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 2px;
	color: #0cd7fc;
	background: url(../images/right/title.png) center center no-repeat;
	background-size: 507px 11px;
}

.map-bg {
	position: absolute;
	/*width: 100%;
	height: 100%;*/
	width: 82px;
	height: 53px;
	/*left: 0;
	top: 0;*/
	/*background: url(../images/right/map-border.png) center center no-repeat;
	background-size: 100% 100%;*/
	z-index: 460;
}

.map-bg1 {
	background: url(../images/right/2212.png) center center no-repeat;
	background-size: 100% 100%;
	top: 0;
	left: 0;
}

.map-bg2 {
	background: url(../images/right/2213.png) center center no-repeat;
	background-size: 100% 100%;
	top: 0;
	right: 0;
}

.map-bg3 {
	background: url(../images/right/2214.png) center center no-repeat;
	background-size: 100% 100%;
	bottom: 0;
	right: 0;
}

.map-bg4 {
	background: url(../images/right/2215.png) center center no-repeat;
	background-size: 100% 100%;
	bottom: 0;
	left: 0;
}

.other-box {
	width: 506px;
}

.other-box > div {
	width: 100%;
	height: 307px;
	background: url(../images/right/data-border.png) center center no-repeat;
	background-size: 100% 100%;
	padding: 10px;
}

.other-top {
	margin-bottom: 20px;
}

.other-top > div, .other-bottom > div {
	position: relative;
	width: 100%;
	height: 100%;
	overflow: hidden;
}

.tab-box {
	width: 100%;
	height: 424px;
	padding: 10px 15px;
	position: relative;
}

.update-time.blue {
	color: #0072fe;
	left: 0;
}

.tab-ul {
	margin-top: 25px;
	height: 45px;
	line-height: 45px;
	border-bottom: 1px solid #002c5b;
}

.tab-ul ul {
	overflow: hidden;
	height: 100%;
}

.tab-ul ul li {
	float: left;
	text-align: center;
	font-size: 16px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	cursor: pointer;
	height: 100%;
}

.tab-ul ul li > span {
	display: inline-block;
	height: 100%;
	color: #0079ff;
	border-bottom: 4px solid transparent;
	transition: all 0.3s ease 0s;
}

.tab-ul ul li.active > span, .tab-ul ul li:hover > span {
	color: #0cd7fc;
	border-bottom: 4px solid #0cd7fc;
}

.col-4 li {
	width: 33.33333333%;
}

.col-2 li {
	width: 20%;
}

.tab-chart {
	height: 310px;
}

.little-title {
	position: absolute;
	top: 10px;
	left: 0;
	width: 100%;
	height: 24px;
	line-height: 24px;
	text-align: center;
	font-size: 16px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 0px;
	color: #98efff;
	background: url(../images/right/8899.png) center center no-repeat;
	background-size: 196px 11px;
}

.flow-chart {
	height: 355px;
	margin-top: 25px;
}

.map-div {
	position: absolute;
	left: 13px;
	top: 60px;
	width: calc(100% - 26px);
	height: calc(100% - 70px);
	background: url(../images/right/map.png) center center no-repeat;
	background-size: 100% 100%;
	z-index: 1;
}

.map-data {
	position: absolute;
	right: 13px;
	top: 15px;
	width: 395px;
	height: 215px;
	background-color: rgba(0, 45, 109, 0.8);
	box-shadow: 0px 1px 4px 0px 
		rgba(0, 46, 88, 0.2);
	border-radius: 3px;
	border: solid 1px #0069ff;
	z-index: 470;
	padding: 8px 15px;
}

input[type=range] {
  -webkit-appearance: none;
  width: 298px;
  border-radius: 3px; /*这个属性设置使填充进度条时的图形为圆角*/
  border: solid 1px #0054c2;
  background: -webkit-linear-gradient(90deg, #7bbcff, #4da4ff) no-repeat, #00234e;/*设置左边颜色为#61bd12，右边颜色为#ddd*/
	background-size: 0% 100%;/*设置左右宽度比例*/
}
input[type=range]::-webkit-slider-runnable-track {
  height: 15px;
  border-radius: 3px; /*将轨道设为圆角的*/
}
input[type=range]:focus {
  outline: none;
}
input[type=range]::-webkit-slider-thumb {
  -webkit-appearance: none;
  height: 21px;
  width: 10px;
  margin-top: -3px; /*使滑块超出轨道部分的偏移量相等*/
  background: #4da4ff; 
  border-radius: 3px; /*外观设置为圆形*/
  border: solid 1px rgba(205, 224, 230, 0.5); /*设置边框*/
  box-shadow: 0 1px 1px #3b4547;

  /*height: 0px;
  width: 0px;
  border: 8px solid red;
  border-bottom-color: #4da4ff;
  border-top: none;
  border-left-color: transparent;
  border-right-color: transparent;
  margin-top: 15px;*/
}

.map-data-box {
	overflow: hidden;
	/*padding: 8px 15px;*/
	padding-bottom: 15px;
}

.map-data-box > input, .map-data-box > .range-box {
	float: left;
	position: relative;
}

.show-min, .show-max {
	position: absolute;
	bottom: -12px;
	font-size: 10px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 1px;
	color: #0069ff;
}

.show-max {
	right: 0;
}

.map-data-val {
	width: 60px;
	height: 26px;
	background-color: #00234e;
	border-radius: 3px;
	border: solid 1px #0054c2;
	font-size: 12px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 1px;
	color: #0cd7fc;
	padding: 0 10px;
	margin-left: 5px;
}

.map-data-name {
	font-family: MicrosoftYaHei;
	font-size: 14px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 1px;
	color: #0069ff;
	margin-bottom: 6px;
}

.other-title {
	font-size: 20px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 2px;
	color: #0cd7fc;
	text-align: center;
	margin: 7px 0 32px 0;
}

.other-data {
	overflow: hidden;
	height: 38px;
	line-height: 38px;
	margin-bottom: 15px;
	background: url(../images/right/124151523.png) center center no-repeat;
	background-size: 100% 100%;
}

.other-data > div {
	float: left;
	height: 100%;
	text-align: center;
	/*border: solid 1px #004fbc;
	background-color: rgba(0, 34, 95, 0.5);*/
	font-size: 16px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 1px;
	color: #5eaeff;
}

.other-data-name {
	width: 106px;
	margin-right: 5px;
	/*border-radius: 5px 0px 0px 5px;*/
}

.other-data-time {
	width: 195px;
	margin-right: 5px;
}

.other-data-val {
	width: 135px;
	/*border-radius: 0px 5px 5px 0px;*/
}

.other-data-box {
	padding: 0 20px;
}

.other-data-val > span {
	font-size: 24px;
	font-weight: normal;
	font-stretch: normal;
	letter-spacing: 2px;
	color: #0cd7fc;
}

.tab-chart > div {
	height: 100%;
	display: none;
}